        <?php
                     foreach($hwCoverage as $i => $details) {
                         $month = $details['month'];
                         $hwCount = round($details['percent']*100, 2);
                         $provCount = array_key_exists($i, $providingCoverage) ? round($providingCoverage[$i]['percent']*100, 2) : 0;
                         $soCount = array_key_exists($i, $soCoverage) ? round($soCoverage[$i]['percent']*100, 2) : 0;
